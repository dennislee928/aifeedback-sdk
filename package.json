{
  "name": "@nics-tw/aifeedback-sdk",
  "version": "1.0.0",
  "description": "A lightweight SDK for submitting feedback.",
  "main": "dist/feedback-sdk.min.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --coverage",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "security:check": "npm audit && echo 'Security check completed'",
    "security:full": "npm audit --audit-level=moderate && echo 'Full security audit completed'",
    "security:trunk": "trunk check --all",
    "security:trunk:fix": "trunk check --all --fix",
    "security:osv": "osv-scanner --lockfile package-lock.json",
    "security:trufflehog": "trufflehog filesystem . --no-verification",
    "security:checkov": "checkov --directory . --framework npm",
    "security:all": "npm run security:audit && npm run security:trunk && npm run security:osv && npm run security:trufflehog && npm run security:checkov",
    "security:ci": "npm run security:audit --audit-level=high && npm run security:trunk && npm run security:osv",
    "security:install": "npm install -g osv-scanner trufflehog checkov",
    "precommit": "npm run lint && npm run format:check && npm run test && npm run security:check"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "sdk",
    "feedback"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^30.0.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
